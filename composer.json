{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "version": "1.3.0",
    "require": {
        "php": "^8.1",
        "ext-gd": "*",
        "barryvdh/laravel-dompdf": "^2.0.3",
        "darkaonline/swagger-lume": "10.*",
        "doctrine/dbal": "^3.2",
        "firebase/php-jwt": "^6.3",
        "friendsofphp/php-cs-fixer": "v3.4.0",
        "guzzlehttp/guzzle": "^7.4.5",
        "illuminate/redis": "^10.14.1",
        "laravel/helpers": "^1.4",
        "laravel/lumen-framework": "^10.0",
        "league/flysystem": "^3.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/oauth2-client": "^2.6",
        "mpdf/mpdf": "^8.1",
        "nunomaduro/collision": "^7.6",
        "psr/container": "1.1.1",
        "yoomoney/yookassa-sdk-php": "3.0.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Src\\": "src/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/BasicFunctions.php",
            "app/Helpers/Callers.php",
            "app/Helpers/Cache.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nexus.tlscontact.com/repository/composer-public"
        },
        {
            "packagist.org": false
        }
    ]
}
